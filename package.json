{
  "name": "frontend-assignment",
  "version": "1.0.0",
  "description": "frontend assignment task js",
  "private": true,
  "scripts": {
    "test": "jest",
    "dev": "webpack serve --mode=development",
    "build": "webpack --mode=production",
    "prettier-run": "prettier --write ./src",
    "lint": "eslint --fix ./src/*.tsx"
  },
  "author": "STP",
  "license": "UNLICENSED",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@types/jest": "^26.0.10",
    "@types/node": "^12.12.5",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-inline-svg-plugin": "^2.1.1",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "serve": "^11.3.0",
    "terser-webpack-plugin": "^2.3.2",
    "typescript": "^3.7.4",
    "webpack": "^5.14.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.(ts|tsx)"
    ],
    "moduleNameMapper": {
      "^[^!].*\\.(css|less)$": "identity-obj-proxy"
    },
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  }
}
